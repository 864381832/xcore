<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="120.0" minWidth="250.0" xmlns="http://javafx.com/javafx/8.0.65"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xwintop.javaFX.jjfx.alert.AlertController">
    <children>
        <VBox fx:id="vbox" alignment="CENTER" layoutX="10.0" layoutY="10.0" spacing="10.0"
              AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <children>
                <Label fx:id="messageLabel" alignment="CENTER" text="asdf" textOverrun="CLIP"
                       wrapText="true">
                    <font>
                        <Font size="15.0"/>
                    </font>
                </Label>
            </children>
        </VBox>
    </children>
</AnchorPane>
